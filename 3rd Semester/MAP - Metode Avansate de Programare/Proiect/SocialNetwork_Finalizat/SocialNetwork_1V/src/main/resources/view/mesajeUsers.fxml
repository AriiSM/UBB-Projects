<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.socialnetwork_1v.controller.Organizat.UTILIZATOR.MesajeController">
   <children>
      <Pane prefHeight="400.0" prefWidth="600.0" style="-fx-background-color: #c0bcbc;">
         <children>
            <TableView fx:id="tableMesaje" prefHeight="283.0" prefWidth="600.0">
              <columns>
                <TableColumn fx:id="coloanaFrom" prefWidth="107.0" style="-fx-background-color: grey;" text="From" />
                <TableColumn fx:id="coloanaTo" prefWidth="107.0" style="-fx-background-color: grey;" text="To" />
                  <TableColumn fx:id="coloanaMesaj" prefWidth="196.5" style="-fx-background-color: grey;" text="Mesaj" />
                  <TableColumn fx:id="coloanaReply" prefWidth="196.5" style="-fx-background-color: grey;" text="Reply" />
                  <TableColumn fx:id="coloadaData" prefWidth="196.5" style="-fx-background-color: grey;" text="Data" />
              </columns>
               <columnResizePolicy>
                  <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
               </columnResizePolicy>
            </TableView>
            <Button layoutX="17.0" layoutY="352.0" mnemonicParsing="false" onAction="#handleTrimite" text="Trimite" />
            <TextField fx:id="lblMesaj" layoutX="17.0" layoutY="302.0" />
            <Button layoutX="114.0" layoutY="352.0" mnemonicParsing="false" onAction="#handleReply" text="Reply" />
         </children>
      </Pane>
   </children>
</AnchorPane>
